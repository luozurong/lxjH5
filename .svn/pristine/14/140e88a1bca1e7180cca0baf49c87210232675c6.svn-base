/*  reset   */
html,body{
    width: 100%;
    height: 100%;
    background:#ffffff;
    overflow-x: hidden;
}
/*  common   */
.clear:after{
    content:'';
    display:block;
    clear:both;
    height:0;
    visibility:hidden;
}
.clear{
    zoom:1;
}
.fl{
    float:left;
}
.fr{
    float:right;
}
/*  body  */
.introduce-clumn{
    width: 100%;
    height: 1.34rem;
    overflow-x: hidden;
    /*border-bottom: 1px solid #d3d3d3;*/
}
.introduce-clumn img{
    width: 100%;
    height: 100%;
    vertical-align: bottom;
    outline: none;
    border: none;
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.clumn-list{
    width: 100%;
}
.list-item{
    position: relative;
    width: 50%;
    height: 1.10rem;
    box-sizing: border-box;
    overflow: hidden;
    background-color:#ffffff;
}
.list-item:nth-child(odd){
    /*border-bottom: 1px solid #d3d3d3;*/
    border-right: 1px solid #E8E8E8;
}
.list-item:nth-child(even){
    /*border-bottom: 1px solid #d3d3d3;*/
}
.list-logo{
    width: 40%;
    height: 1.10rem;
    text-align: center;
}
.list-logo>img{
    width: 0.38rem;
    height: 0.38rem;
    vertical-align: middle;
    outline: none;
    border: none;
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.list-logo>span{
    height: 100%;
    display: inline-block;
    vertical-align: middle;
}
.list-info{
    width: 60%;
    height: 1.10rem;
    color: #6a6a6a;
    text-align: left;

}
.list-info>h1{
    padding-top: 0.34rem;
    font-size: 0.14rem;
    width: 0.92rem;
    /* height: 0.14rem; */
    /* line-height: 0.14rem; */
    font-weight: 200;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;

}
.list-info>p{
    padding-top: 0.12rem;
    font-size: 0.10rem;
    width: 0.92rem;
    min-height: 0.10rem;
    line-height: 0.13rem;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.bottom-border {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 1px;
    background-color: #E8E8E8;
    z-index: 999;
}
.activitylose{
	margin-top: 1.34rem;
}

.wrapper-second{
	top: 1.33rem;
	background-color: #F0F0F0;
}
.clumn-list{
	border-top: 1px solid #E8E8E8;
	
}
.box-wrap{
	position: absolute;
	width: 100%;
	height: 1.34rem;
	top: 0;
	z-index: 222;
}


#pullUp {
	background: #fff;
	text-align: center;
	height: 40px;
	line-height: 40px;
	padding: 5px 10px;
	border-bottom: 1px solid #E8E8E8;
	font-weight: bold;
	font-size: 12px;
	color: #888;
	position: initial;
	width: auto;
}
/*#pullDown .pullDownIcon,*/

#pullUp .pullUpIcon {
	display: block;
	margin: 0 auto;
	width: 40px;
	height: 40px;
	background: url(../images/pull-icon@2x.png) 0 0 no-repeat;
	-webkit-background-size: 40px 80px;
	background-size: 40px 80px;
	-webkit-transition-property: -webkit-transform;
	-webkit-transition-duration: 250ms;
}
.noData{
	display: none;
	position: absolute;
	bottom: -40px;
	width: 100%;
	background-color: #F0F0F0;
	text-align: center;
	line-height: 40px;
	color: #888888;
}




*{
	padding: 0;
	margin: 0;
}
.commander-content{
	width: 100%;
}
.commander-content p img{
	width: 100%;
	display: block;
	margin-bottom: 0.42rem;
}
.commander-div{
	display: block;
	height: 0.44rem;
}
.commander-submit{
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 1;
	width: 100%;
}
.commander-submit span{
	display: block;
	background: #fc9153;
	color: #fff;
	font-size: 0.16rem;
	line-height: 0.44rem;
	box-sizing: border-box;
	text-align: center;
}

.clear:after{
    content:'';display:block;clear:both;height:0;visibility:hidden;
}
.clear{
    zoom:1;
}
#thelist {
    background: #fff;
}
.add-maintainlist{
    width: 100%;
	overflow: hidden;
	background: #fff;
	padding: 0.05rem 0.05rem 0.05rem 0;
	margin-left: 0.1rem;
	/*padding: 0.05rem 0.10rem;*/
	border-bottom: 1px solid #e8e8e8;
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-webkit-tap-highlight-color: rgba(255,255,255,0);
}

.add-maintainlist .picture {
	width: 30%;
	float: left;
	height: 0.9rem;
}

.picture img {
	width: 100%;
	height: 100%;
    vertical-align: top;
}
.picture span {
    height: 100%;
    display: inline-block;
    vertical-align: top;
}

.add-maintainlist .info {
	width: 70%;
	height: .9rem;
	float: left;
	position: relative;
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	padding-left: .1rem;
	padding-right: 0.1rem;
}

ul li .price {
	font-size: 0.12rem;
	color: #ff661b;
	/*font-weight: 500;*/
	position: absolute;
	bottom: 0;
}
.add-maintainlist{
	-webkit-tap-highlight-color: rgba(255,255,255,0);
}
.info .title {
	font-size: 0.12rem;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}

.info .introduce {
	/*text-align: Justify;
	text-justify: inter-ideograp;*/
	font-size: 0.1rem;
	line-height: .16rem;
	padding-top: 0.05rem;
	color: #888;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}


body{
    width:100%;
    min-width:100%;
}
li{
    list-style: none;
}
#productevaluate{
    width:100%;
}
.productevaluatebox{
    padding-left: 0.113rem;
    background: #fff;
    margin-bottom: 0.08rem;
    padding-bottom: 0.231rem;
}
.headPortrait{
    width:0.3rem;
    height:0.3rem;
    display:inline-block;
    vertical-align: bottom;
    text-overflow : clip;
    border-radius: 50%;
    float: left;
}
.evaluateName{
    line-height:0.3rem;
    height: 0.3rem;
    margin-left: 0.4rem;
    color: #222;
    font-size: 0.118rem;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.evaluateTime{
    line-height:0.24rem;
    color:#666;
    /*font-size:0.14rem;*/
}
.evaluateContents{
    margin-right: 0.1rem;
    margin-bottom: 0.1545rem;
    font-size:0.14rem;
}
.evaluateBox{
    padding-top:0.04rem;
}
#evaluatePhotoes{
    margin-left: 0.1rem;
}
.evaluatePhotoes{
    display:inline-block;
}
.commentReply{
    background:#efeff4;
    width:80%;
    height:1rem;
    margin-top:0.1rem;
}
.commentImages,.replyImages{
    /*width:1rem;*/
    display:inline-block;
}
.replyContent{
    width: 95%;
    background: #efeff4;
    padding-top: 0.07729rem;
    padding-bottom: 0.07729rem;
    font-size: 0.118rem;
    padding-left: 0.07749rem;
}
.sanjiao{
    margin-left: 0.3rem;
    position: relative;
    top: 0.08rem;
}
pre{
    white-space: pre-wrap;
}
.commentImages img{
    display: inline-block;
    width:68px;
    height:68px;
    border:0;
    opacity: 0;
}
.replyImages img{
    display: inline-block;
    width:68px;
    height:68px;
    border:0;
    opacity: 0;
}
.product_img{
    width: 68px;
    margin-right: 0.1rem;
    height: 68px;
    background-repeat: no-repeat;
    background-position:center center;
    background-size:cover;
}

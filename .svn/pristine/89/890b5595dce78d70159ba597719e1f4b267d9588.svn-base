		<script type="text/javascript">
			var u = navigator.userAgent;
			var clientType = GetURLParameter('clientType');
			if(!clientType) {
				if(!!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/)) {
					clientType = "ios";
				} else {
					clientType = "android";
				}
			}
			var comefromType = GetURLParameter("from") ? GetURLParameter("from") : '';
			var baiduButtonId = 3076;
			var baiduId = 3076;
			if(comefromType == "kaimen") {
				baiduId = baiduId + 1;
			} else if(comefromType == "shengh") {
				baiduId = baiduId + 2;
			} else if(comefromType == "zixun") {
				baiduId = baiduId + 3;
			} else if(comefromType == "tanc") {
				baiduId = baiduId + 4;
			} else if(comefromType == "fuc") {
				baiduId = baiduId + 5;
			} else if(comefromType == "sc") {
				baiduId = baiduId + 6;
			} else if(comefromType == "push") {
				baiduId = baiduId + 7;
			} else if(comefromType == "feed") {
				baiduId = baiduId + 8;
			} else if(comefromType == "shenghz") {
				baiduId = baiduId + 9;
			} else if(comefromType == "huod") {
				baiduId = baiduId + 10;
			} else {
				baiduId = baiduId;
			}
			var _hmt = _hmt || [];
			_hmt.push(['_setAutoPageview', false]);
			_hmt.push(['_trackPageview', '/client=' + clientType + '-幸福集市20-活动ID=' + baiduId]);
			(function() {
				var hm = document.createElement("script");
				hm.src = "https://hm.baidu.com/hm.js?245cd6becb25900dbb7f9f62761431d7";
				var s = document.getElementsByTagName("script")[0];
				s.parentNode.insertBefore(hm, s);
			})();
		</script>
		<script type="text/javascript">
			(function(doc, win) {
				var docEl = doc.documentElement,
					isIOS = navigator.userAgent.match(/iphone|ipod|ipad/gi),
					dpr = isIOS ? Math.min(win.devicePixelRatio, 3) : 1,
					dpr = window.top === window.self ? dpr : 1, //被iframe引用时，禁止缩放
					scale = 1 / dpr,
					resizeEvt = 'orientationchange' in window ? 'orientationchange' : 'resize';
				docEl.dataset.dpr = dpr;
				var recalc = function() {
					var width = docEl.clientWidth;
					if(width / dpr > 768) {
						width = 768 * dpr;
					}
					docEl.style.fontSize = 100 * (width / 414) + 'px';
				};
				recalc();
				if(!doc.addEventListener) return;
				win.addEventListener(resizeEvt, recalc, false);
			})(document, window);
		</script>
